{
  "mcpServers": {
    "container-use": {
      "type": "stdio",
      "command": "container-use",
      "args": ["stdio"],
      "env": {}
    },
    "typescript": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@mizchi/lsmcp", "-p", "tsgo"],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-playwright"],
      "env": {}
    }
  }
}
